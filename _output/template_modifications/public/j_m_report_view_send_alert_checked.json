{
    "template": "report_view",
    "description": "Sets the checked attribute of the report resolution/rejection alert checkbox",
    "execution_order": 10,
    "enabled": true,
    "action": "preg_replace",
    "find": "/(<xf:option name=\"send_alert\" .+)(>)/isU",
    "replace": "$1 checked=\"checked\"$2"
}